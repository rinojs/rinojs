{
  "name": "rinojs",
  "version": "2.13.3",
  "description": "Fast learning, preprocessing, intuitive web framework",
  "main": "dist/index.js",
  "bin": {
    "rinojs": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "test-cp": "npx cypress open",
    "test-pw": "npx playwright test",
    "test-dev": "cd test-project && node dev.js",
    "test-generate": "cd test-project && node generate.js",
    "test-sitemap": "cd test-project && node sitemap.js",
    "test-feed": "cd test-project && node feed.js",
    "test-ssr": "cd test-project && node test-ssr.js",
    "test-backoffice": "cd test-project && node backoffice.js",
    "pack": "node pack/index.js",
    "bundle": "rollup -c",
    "release": "npm run pack && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rinojs/rinojs.git"
  },
  "keywords": [
    "rino",
    "templating",
    "static-site-generator",
    "static-site-generation",
    "static-site-building",
    "frontend-development",
    "rinojs",
    "webframework"
  ],
  "author": "Victor Chanil Park",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rinojs/rinojs/discussions"
  },
  "homepage": "https://rinojs.org/",
  "dependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-virtual": "^3.0.2",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "clean-css": "^5.3.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "fs-extra": "^11.3.0",
    "markdown-it": "^14.1.0",
    "multer": "^1.4.5-lts.2",
    "rollup": "^4.35.0",
    "sharp": "^0.33.5",
    "totjs": "^2.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "ws": "^8.18.1"
